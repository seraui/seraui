      "path": "action.tsx",
      "content": "import Button from \"./button\"\r\nimport {\r\n  DownloadIcon,\r\n  ShoppingCartIcon,\r\n  HeartIcon,\r\n  StarIcon,\r\n  ShareIcon,\r\n} from \"./icons\"\r\n\r\nconst ActionButton = () => {\r\n    return (\r\n      <div className=\"flex w-full flex-col items-center justify-center gap-8\">\r\n          {/* Action Buttons */}\r\n          <div className=\"flex flex-col gap-4\">\r\n            \r\n            <div className=\"flex flex-wrap items-center justify-center gap-4\">\r\n                <Button variant=\"default\" iconLeft={<DownloadIcon />}>\r\n                    Download\r\n                </Button>\r\n                <Button variant=\"default\" iconLeft={<ShoppingCartIcon />}>\r\n                    Add to Cart\r\n                </Button>\r\n                <Button variant=\"outline\" iconLeft={<HeartIcon />}>\r\n                    Add to Wishlist\r\n                </Button>\r\n                <Button variant=\"secondary\" iconLeft={<StarIcon />}>\r\n                    Rate Product\r\n                </Button>\r\n                <Button variant=\"ghost\" iconLeft={<ShareIcon />}>\r\n                    Share\r\n                </Button>\r\n            </div>\r\n          </div>\r\n      </div>\r\n    );\r\n  }\r\n  \r\n  export default ActionButton;\r\n  ",
      "type": "registry:ui"
    },
